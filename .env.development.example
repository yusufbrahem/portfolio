# Development Environment Variables
# Copy this file to .env.local or .env.development.local for local development
# This file is safe to commit as it contains no secrets

# Database Configuration
# For local PostgreSQL development
DATABASE_URL="postgresql://postgres:password@localhost:5432/portfolio"

# Admin Panel Security
# Use a strong password for local development
# IMPORTANT: This should be different from production password!
ADMIN_PASSWORD="dev-password-change-me"

# Next.js Environment
NODE_ENV="development"

# Optional: Site URL for local development
# NEXT_PUBLIC_SITE_URL="http://localhost:3000"
